<%= f.input :start_time_date, :as => :date_picker, :input_html => {:value => @subscription.start_time.try(:strftime,'%m/%d/%Y'), :style => "width: 105px"}, :label => "Start Date", :wrapper_html => {:class => "inline"} %>
<%= f.input :start_time_time, :collection => appointment_times(:start_time => 9.hours, :end_time => 17.hours, :exact => false), :selected => f.object.start_time_time, :label => "Start Time", :wrapper_html => {:class => "inline"}, :input_html => {:style => 'width: 230px'} %>
